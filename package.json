    "url": "https://github.com/davidfunk13/company-website/issues"
  },
  "homepage": "https://github.com/davidfunk13/company-website#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "concurrently": "^5.0.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0"
  }
